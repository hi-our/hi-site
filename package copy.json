{
  "name": "quickly-mask",
  "version": "1.0.0",
  "description": "Example on using create-react-app with a Node Express Backend",
  "author": {
    "name": "shenghanqin",
    "url": "https://xiaoxili.com"
  },
  "license": "MIT",
  "scripts": {
    "client": "cd client && yarn start",
    "server": "nodemon server.js",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "dev:server": "cd client && yarn build && cd .. && yarn start",
    "start": "NODE_ENV=production node server.js",
    "dev:server:taro": "cd taro && npm run release:h5 && cd .. && yarn start:taro",
    "start:taro": "NODE_ENV=production node server-taro.js",
    "heroku-postbuild": "cd client && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "babel-eslint": "^8.2.1",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "2.9.0",
    "eslint-config-standard": "11.0.0",
    "eslint-plugin-import": "2.9.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "3.7.0",
    "eslint-plugin-standard": "^3.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shenghanqin/quickly-mask.git"
  },
  "bugs": {
    "url": "https://github.com/shenghanqin/quickly-mask/issues"
  },
  "keywords": [
    "node",
    "express",
    "create-react-app"
  ],
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "request": "^2.88.0",
    "tencentcloud-sdk-nodejs": "^3.0.126"
  }
}
